platform :ios, '9.0'
use_frameworks!

target 'GolfPS' do
    pod 'GoogleMaps'
    pod 'GooglePlaces'
    pod 'Firebase/Auth'
    pod 'Firebase/Firestore'
    pod 'Firebase/Performance'
    pod 'Firebase/Core'
    pod 'Firebase/Crashlytics'

    pod 'SnapSDK'
end

